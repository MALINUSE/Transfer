<p id="notice"><%= notice %></p>

<h1>Listing People Sims</h1>

<table>
  <thead>
    <tr>
      <th>People</th>
      <th>Sim carte</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @people_sims.each do |people_sim| %>
      <tr>
        <td><%= people_sim.people %></td>
        <td><%= people_sim.sim_carte %></td>
        <td><%= link_to 'Show', people_sim %></td>
        <td><%= link_to 'Edit', edit_people_sim_path(people_sim) %></td>
        <td><%= link_to 'Destroy', people_sim, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New People sim', new_people_sim_path %>
