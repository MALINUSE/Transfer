<div class="col-sm-10">

  <div class="page-header">
    <h1>Listing Customs</h1>

    <div class="contextual">

    </div>
  </div>

  <table class="table table-striped table-bordered table-condensed" id="people">
    <thead>
    <tr class="color3">
      <th colspan="1"></th>
      <th colspan="1">Codecustom</th>
      <th colspan="1">First name</th>
      <th colspan="1">Last name</th>
      <th colspan="1">Sexe</th>
      <th colspan="1">Country</th>
      <th colspan="1">City</th>
    </tr>
    </thead>
    <tbody>
    <% @i=1
       @people.each do |custom|
    %>
        <tr>
          <td><%= @i %></td>
          <td><%= link_to custom.codecustom, custom %></td>
          <td><%= custom.first_name %></td>
          <td><%= custom.last_name %></td>
          <td><%= custom.sexe %></td>
          <td><%= custom.country %></td>
          <td><%= custom.city %></td>
        </tr>
    <% @i+=1
       end %>
    </tbody>
  </table>
</div>
</div><!-- /.col-sm-10 -->

